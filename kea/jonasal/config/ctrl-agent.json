{
  "Control-agent": {
    "control-sockets": {
      "dhcp4": {
        "socket-name": "/kea/sockets/dhcp4.socket",
        "socket-type": "unix"
      },
      "dhcp6": {
        "socket-name": "/kea/sockets/dhcp6.socket",
        "socket-type": "unix"
      }
    },
    "hooks-libraries": [ ],
    "http-host": "0.0.0.0",
    "http-port": 8000,
    "loggers": [
      {
        "debuglevel": 0,
        "name": "kea-ctrl-agent",
        "output_options": [
          {
            "flush": true,
            "output": "stdout",
            "pattern": "%D{%Y-%m-%d %H:%M:%S.%q} %-5p [%c/%i.%t] %m\n"
          },
          {
            "flush": false,
            "maxsize": 1048576,
            "maxver": 8,
            "output": "/kea/logs/ctrl-agent.log",
            "pattern": "%D{%Y-%m-%d %H:%M:%S.%q} %-5p [%c/%i.%t] %m\n"
          }
        ],
        "severity": "INFO"
      }
    ]
  }
}